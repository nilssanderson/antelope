=======================================\n\nbutton,\n.button,\n.btn {\n  background-color: $button-background-color;\n  border-color: $button-border-color;\n  border-style: $button-border-style;\n  border-width: $button-border-width;\n  border-radius: $button-radius;\n  color: $button-text-color;\n  cursor: pointer;\n  display: inline-block;\n  font-family: $button-font-family;\n  font-size: $button-font-size;\n  margin: $button-margin;\n  overflow: hidden;\n  padding: $button-padding;\n  position: relative;\n\n  &:hover {\n    background-color: $button-hover-background-color;\n    color: $button-hover-text-color;\n  }\n\n  &.l {\n    float: left;\n  }\n\n  &.positive { }\n}\n\n\n// =============================================================================\n// Buttons Code - Alternatives\n// =============================================================================\n\nbutton,\n.btn {\n  &.fancy {\n    background-color: $button-fancy-background-color;\n    color: $button-fancy-text-color;\n    -webkit-transition: border-color 0.3s, color 0.3s;\n    transition: border-color 0.3s, color 0.3s;\n    -webkit-transition-timing-function: cubic-bezier(0.2, 1, 0.3, 1);\n    transition-timing-function: cubic-bezier(0.2, 1, 0.3, 1);\n\n  \t&:before {\n      content: '';\n  \t\tposition: absolute;\n  \t\ttop: -20%;\n  \t\tleft: -20%;\n  \t\twidth: 150%;\n  \t\theight: 100%;\n  \t\tbackground: lighten($button-fancy-hover-background-color, 10%);\n  \t\tz-index: 1;\n  \t\t-webkit-transform: rotate3d(0, 0, 1, -45deg) translate3d(0, -3em, 0);\n  \t\ttransform: rotate3d(0, 0, 1, -45deg) translate3d(0, -3em, 0);\n  \t\t-webkit-transform-origin: 0% 100%;\n  \t\ttransform-origin: 0% 100%;\n  \t\t-webkit-transition: -webkit-transform 0.3s, opacity 0.3s, background-color 0.3s;\n  \t\ttransition: transform 0.3s, opacity 0.3s, background-color 0.3s;\n  \t}\n\n  \t.text {\n  \t\tposition: relative;\n  \t\tz-index: 2;\n  \t}\n\n  \ti {\n  \t\tfont-size: 0;\n  \t\tpadding-left: 0;\n  \t\t-webkit-transition: all .2s ease-in-out;\n  \t\ttransition: all .2s ease-in-out;\n  \t\tposition: relative;\n  \t\tz-index: 2;\n  \t}\n\n  \t&:hover,\n  \t&.current {\n      background-color: $button-fancy-background-color;\n  \t\tborder-color: $button-fancy-hover-background-color;\n  \t\tcolor: $button-fancy-hover-text-color;\n\n  \t\t&:before {\n        top: 0;\n        left: 0;\n  \t\t\topacity: 1;\n  \t\t\tbackground: $button-fancy-hover-background-color;\n  \t\t\t-webkit-transform: rotate3d(0, 0, 1, 0deg);\n  \t\t\ttransform: rotate3d(0, 0, 1, 0deg);\n  \t\t\t-webkit-transition-timing-function: cubic-bezier(0.2, 1, 0.3, 1);\n  \t\t\ttransition-timing-function: cubic-bezier(0.2, 1, 0.3, 1);\n  \t\t}\n\n  \t\ti {\n  \t\t\tfont-size: 90%;\n  \t\t\tpadding: 0 .5em;\n\n        &:first-child {\n    \t\t\tpadding: 0 .5em 0 0;\n        }\n\n        &:last-child {\n    \t\t\tpadding: 0 0 0 .5em;\n        }\n  \t\t}\n  \t}\n  }\n}","syntax":"scss","file":"_buttons.scss","parentReference":"2","wrapper":"<sg-wrapper-content/>","renderMarkup":"<button>Button</button>\n<button class=\"fancy\">\n  <span class=\"text\">Button</span>\n  <i class=\"fa fa-facebook\"></i>\n</button>","variables":["button-background-color","button-border-color","button-border-style","button-border-width","button-radius","button-text-color","button-font-family","button-font-size","button-margin","button-padding","button-hover-background-color","button-hover-text-color","button-fancy-background-color","button-fancy-text-color","button-fancy-hover-background-color","button-fancy-background-color","button-fancy-hover-background-color","button-fancy-hover-text-color","button-fancy-hover-background-color"],"fileHash":"2aac34d396503e6bc15d3cea15df8357"},{"header":"Carousel","description":"","modifiers":[],"deprecated":false,"experimental":false,"reference":"2.2","markup":"markup:","css":"// =============================================================================\n// Carousel Code\n// =================