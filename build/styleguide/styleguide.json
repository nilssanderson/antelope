ax":"scss","file":"_overview.scss","wrapper":"<sg-wrapper-content/>","fileHash":"414a4e846d308f39cc74d6fbf8ed5ef9"},{"header":"Colors","description":"","modifiers":[],"deprecated":false,"experimental":false,"reference":"3.1","markup":"markup:","css":"// =============================================================================\n// Colors Code\n// =============================================================================\n\n.bg-white { background-color: $white !important; }\n.bg-black { background-color: $black !important; }\n.bg-red { background-color: $red !important; }\n.bg-pink { background-color: $pink !important; }\n.bg-purple { background-color: $purple !important; }\n.bg-deep-purple { background-color: $deep-purple !important; }\n.bg-indigo { background-color: $indigo !important; }\n.bg-blue { background-color: $blue !important; }\n.bg-light-blue { background-color: $light-blue !important; }\n.bg-cyan { background-color: $cyan !important; }\n.bg-teal { background-color: $teal !important; }\n.bg-green { background-color: $green !important; }\n.bg-light-green { background-color: $light-green !important; }\n.bg-lime { background-color: $lime !important; }\n.bg-yellow { background-color: $yellow !important; }\n.bg-amber { background-color: $amber !important; }\n.bg-orange { background-color: $orange !important; }\n.bg-deep-orange { background-color: $deep-orange !important; }\n.bg-brown { background-color: $brown !important; }\n.bg-grey { background-color: $grey !important; }\n.bg-blue-grey { background-color: $blue-grey !important; }","syntax":"scss","file":"_colors.scss","parentReference":"3","wrapper":"<sg-wrapper-content/>","renderMarkup":"markup:","variables":["white","black","red","pink","purple","deep-purple","indigo","blue","light-blue","cyan","teal","green","light-green","lime","yellow","amber","orange","deep-orange","brown","grey","blue-grey"],"fileHash":"0484887ea36f9d312965a1cf6922eaa1"},{"header":"Functions","description":"","modifiers":[],"deprecated":false,"experimental":false,"reference":"3.2","markup":"markup:","css":"// =============================================================================\n// Functions Code\n// =============================================================================","syntax":"scss","file":"_functions.scss","parentReference":"3","wrapper":"<sg-wrapper-content/>","renderMarkup":"markup:","variables":[],"fileHash":"28acf724629dd0c427c22e9639ecccc8"},{"header":"Helpers","description":"","modifiers":[],"deprecated":false,"experimental":false,"reference":"3.3","markup":"markup:","css":"// =============================================================================\n// Helpers Code\n// =============================================================================","syntax":"scss","file":"_helpers.scss","parentReference":"3","wrapper":"<sg-wrapper-content/>","renderMarkup":"markup:","variables":[],"fileHash":"e3bc07e423ac2517f97b807da0ccf4f5"},{"header":"Mixins","description":"","modifiers":[],"deprecated":false,"experimental":false,"reference":"3.4","markup":"markup:","css":"// =============================================================================\n// Mixins Code\n// =============================================================================\n\n// Media queries\n// Breakpoints for each query\n@mixin respondTo($media) {\n  @if $media == smartphone {\n    @media only screen and (max-width: $smartphone) { @content; }\n  }\n  @else if $media == tablet {\n    @media only screen and (min-width: $tabletPortrait) and (max-width: $tabletLandscape) { @content; }\n  }\n  @else if $media == smallScreen {\n    @media only screen and (max-width: $desktop) { @content; }\n  }\n  @else if $media == desktop {\n    @media only screen and (min-width: $desktop) { @content; }\n  }\n  @else if $media == largeScreen {\n    @media only screen and (min-width: $largeScreen) { @content; }\n  }\n}\n\n.mobile {\n  @include respondTo(smartphone) {\n    background: $red;\n  }\n}\n.tablet {\n  @include respondTo(tablet) {\n    background: $blue;\n  }\n}\n.smallScreen {\n  @include respondTo(smallScreen) {\n    background: $yellow;\n  }\n}\n.desktop {\n  @include respondTo(desktop) {\n    background: $green;\n  }\n}\n.largeScreen {\n  @include respondTo(largeScreen) {\n    background: $cyan;\n  }\n}","syntax":"scss","file":"_mixins.scss","parentReference":"3","wrapper":"<sg-wrapper-content/>","renderMarkup":"markup:","variables":["media","media","smartphone","media","tabletPortrait","tabletLandscape","media","desktop","media","desktop","media","largeScreen","red","blue","yellow","green","cyan"],"fileHash":"9d0c1f1df1d6238ead09431174467db4"},{"header":"Placeholders","description":"","modifiers":[],"deprecated":false,"experimental":false,"reference":"3.5","markup":"markup:","css":"// =============================================================================\n// Placeholders Code\n// =============================================================================","syntax":"scss","file":"_placeholders.scss","parentReference":"3","wrapper":"<sg-wrapper-content/>","renderMarkup":"markup:","variables":[],"fileHash":"4dd946274af7c2602ba79fea23b7b5ed"},{"header":"Variables","description":"","modifiers":[],"deprecated":false,"experimental":false,"reference":"3.6","markup":"markup:","css":"// =============================================================================\n// Variables Code\n// =============================================================================","syntax":"scss","file":"_variables.scss","parentReference":"3","wrapper":"<sg-wrapper-content/>","renderMarkup":"markup:","variables":[],"fileHash":"d56a0cb230d721b1fd24a5dd63c84d57"},{"header":"Footer","description":"","modifiers":[],"deprecated":false,"experimental":false,"reference":"4.1","markup":"markup:","css":"// =============================================================================\n// Footer Code\n// =============================================================================","syntax":"scs